<?xml version='1.0' encoding='utf-8'?>
<interface version="1.2">
  <object class="tk.Frame" id="settingsFrame">
    <property name="background">#e8e9f2</property>
    <property name="height">400</property>
    <property name="width">700</property>
    <layout manager="pack">
      <property name="expand">true</property>
      <property name="fill">both</property>
      <property name="side">top</property>
    </layout>
    <containerlayout manager="pack">
      <property name="propagate">False</property>
    </containerlayout>
    <child>
      <object class="tk.Frame" id="frame37">
        <property name="background">#e8e9f2</property>
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="fill">both</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="tk.Label" id="settingsLabel">
            <property name="anchor">w</property>
            <property name="background">#e8e9f2</property>
            <property name="font">{@Yu Gothic UI Semilight} 14 {}</property>
            <property name="justify">left</property>
            <property name="text" translatable="yes">Settings</property>
            <layout manager="pack">
              <property name="fill">x</property>
              <property name="padx">10</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Frame" id="frame1">
            <property name="background">#e8e9f2</property>
            <property name="height">200</property>
            <property name="relief">flat</property>
            <property name="width">200</property>
            <layout manager="pack">
              <property name="fill">x</property>
              <property name="padx">10</property>
              <property name="pady">10</property>
              <property name="side">top</property>
            </layout>
            <child>
              <object class="tk.Label" id="label1">
                <property name="background">#e8e9f2</property>
                <property name="font">{Yu} 11 {}</property>
                <property name="text" translatable="yes">language</property>
                <layout manager="pack">
                  <property name="side">left</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Button" id="languageButtonRu">
                <property name="font">{Yu} 12 {}</property>
                <property name="relief">flat</property>
                <property name="text" translatable="yes">Русский</property>
                <layout manager="pack">
                  <property name="padx">10</property>
                  <property name="side">left</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Button" id="languageButtonEn">
                <property name="font">{Yu} 12 {}</property>
                <property name="relief">flat</property>
                <property name="text" translatable="yes">English</property>
                <layout manager="pack">
                  <property name="side">left</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
